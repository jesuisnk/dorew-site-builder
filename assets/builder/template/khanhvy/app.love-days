html,body{background-color:#f6f6f6;margin:0;padding:0}body,a:hover,textarea,input{cursor:url(https://dorew-site.github.io/assets/ring/body/body.cur),default!important;}img{max-width:100%}body{display:grid;font-family:"Segoe UI",sans-serif;grid-template-columns:repeat(12,1fr);grid-auto-rows:minmax(auto,auto);color:#3e3e3e;text-shadow:.4px .4px .4px rgba(0,0,0,.2)}h1{font-weight:lighter;grid-column:span 3;text-align:center;color:#ec407a}#wrapper{display:grid;grid-column:3 / 11;grid-template-columns:repeat(3,1fr);grid-auto-rows:minmax(auto,auto);grid-row-gap:10px;margin-top:3.6rem}#clock-box,#info{grid-column:span 3;display:grid;grid-template-columns:repeat(12,1fr)}#clock-box #clock{grid-column:4 / 10;padding:.6rem 2rem;padding-top:.8rem;text-align:center;font-size:2.4rem;border-radius:10rem}date,time{display:block}date{font-size:18px}time{font-family:"Arial",sans-serif;line-height:3.2rem;letter-spacing:2px}.avt{width:160px;height:160px;border-radius:50%;box-shadow:2px 2px 8px rgba(0,0,0,.2)}#info{margin-top:2.6rem}#heart{grid-column:span 2;display:grid;grid-template-rows:2;font-size:44px;color:#b90d46;align-self:center;padding-bottom:2.6rem;text-align:center}anni{font-size:16px;letter-spacing:.2px}#info .one{grid-column:3 / 6;text-align:center}#info .two{grid-column:8 / 11;text-align:center}footer{grid-column:span 3;text-align:center;font-size:1.6rem;font-weight:lighter;margin-top:1.2rem}#music{grid-column:2;position:relative;z-index:1000;opacity:.4}#mask{background-image:linear-gradient(to top,#6d44cc5d 0%,#f09cd880 100%);opacity:.42;width:100vw;height:100vh;position:fixed;top:0;left:0}@media (max-width: 992px){ #wrapper{grid-column:span 12}#clock-box #clock{grid-column:2 / 12}.avt{width:120px;height:120px}#heart{grid-column:span 12;padding-bottom:1rem}footer{font-size:1.2rem;margin:0;padding-bottom:2rem}}span.bietdanh{font-size:1.3rem;color:#eee;text-shadow:1px 1px 3px #eee;font-family:'Dancing Script',cursive}.hello{opacity:1!important}.full{position:fixed;left:0;top:0;width:100%;height:100%;z-index:1;z-index:9999999999999}.full .content{background-color:rgba(0,0,0,0.75)!important;height:100%;width:100%;display:grid}.full .content img{left:50%;transform:translate3d(0,0,0);animation:zoomin 1s ease;max-width:100%;max-height:100%;margin:auto}.byebye{}.byebye:hover{transform:scale(0.8)!important}.gallery{display:grid;grid-column-gap:8px;grid-row-gap:8px;grid-template-columns:repeat(auto-fill,minmax(250px,1fr))}.gallery img{max-width:100%;border-radius:8px;box-shadow:0 0 16px #333;transition:all 1.5s ease}.gallery img:hover{box-shadow:0 0 32px #333}.gallery .content{padding:4px}.gallery .gallery-item{transition:grid-row-start 300ms linear;transition:transform 300ms ease;transition:all .5s ease;cursor:pointer}.gallery .gallery-item:hover{transform:scale(1.025)}@media (max-width: 600px){.gallery{grid-template-columns:repeat(auto-fill,minmax(30%,1fr))}}@media (max-width: 400px){.gallery{grid-template-columns:repeat(auto-fill,minmax(50%,1fr))}}@-moz-keyframes zoomin{0%{max-width:50%;transform:rotate(-30deg);filter:blur(4px)}30%{filter:blur(4px);transform:rotate(-80deg)}70%{max-width:50%;transform:rotate(45deg)}100%{max-width:100%;transform:rotate(0deg)}}@-webkit-keyframes zoomin{0%{max-width:50%;transform:rotate(-30deg);filter:blur(4px)}30%{filter:blur(4px);transform:rotate(-80deg)}70%{max-width:50%;transform:rotate(45deg)}100%{max-width:100%;transform:rotate(0deg)}}@-o-keyframes zoomin{0%{max-width:50%;transform:rotate(-30deg);filter:blur(4px)}30%{filter:blur(4px);transform:rotate(-80deg)}70%{max-width:50%;transform:rotate(45deg)}100%{max-width:100%;transform:rotate(0deg)}}@keyframes zoomin{0%{max-width:50%;transform:rotate(-30deg);filter:blur(4px)}30%{filter:blur(4px);transform:rotate(-80deg)}70%{max-width:50%;transform:rotate(45deg)}100%{max-width:100%;transform:rotate(0deg)}}.hidebg{background:url(https://i.imgur.com/t33rWxC.png) center no-repeat;width:90%;margin:0 auto;height:2rem;grid-column:span 4}a{text-decoration:none}#mask{pointer-events:none}#heart{text-shadow:#fff 0 0 2px,#fff 0 0 2px,#fff 0 0 2px,#fff 0 0 2px,#fff 0 0 2px,#fff 0 0 2px,#fff 0 0 2px,#fff 0 0 2px;-webkit-animation:hue 5s linear infinite}#clock-box{background:url(https://i.imgur.com/sB3f5rL.png) center;width:89%;margin:0 auto}#album-box{background:url(https://i.imgur.com/E9kbT5e.png) center;width:90%;margin:0 auto;height:37px;grid-column:span 4;background-size:contain}#album-box2{background:url(https://i.imgur.com/SnOkZRI.png) center;width:90%;margin:0 auto;height:39px;grid-column:span 4;background-size:contain}date,time{display:block;font-size:.8rem}#clock{background:url(https://i.imgur.com/t33rWxC.png) bottom no-repeat;text-align:center;border-radius:9rem;height:89px;color:#fff;text-shadow:#dd057f 0 0 2px,#dd057f 0 0 2px,#dd057f 0 0 2px,#dd057f 0 0 2px,#dd057f 0 0 2px,#dd057f 0 0 2px,#dd057f 0 0 2px,#dd057f 0 0 2px}.m{color:#fff;font-size:1.5rem;font-family:'Dancing Script',cursive;text-shadow:#2b31ff 0 0 2px,#2b31ff 0 0 7px,#2b31ff 0 0 12px,#2b31ff 0 0 17px,#2b31ff 0 0 7px,#2b31ff 0 0 7px,#2b31ff 0 0 7px,#2b31ff 0 0 7px}.zh{color:#fff;font-size:1.5rem;font-family:'Dancing Script',cursive;text-shadow:#fb00ff 0 0 2px,#fb00ff 0 0 7px,#fb00ff 0 0 12px,#fb00ff 0 0 17px,#fb00ff 0 0 7px,#fb00ff 0 0 7px,#fb00ff 0 0 7px,#fb00ff 0 0 7px}::-webkit-scrollbar{height:13px;width:13px}::-webkit-scrollbar-thumb{-webkit-box-shadow:inset 2px 2px 3px #696969;background:#696969;border:1px solid #D5D4D4;border-radius:5px;-moz-border-radius:5px;-webkit-border-radius:5px}::-webkit-scrollbar-track{-webkit-box-shadow:inset 2px 2px 3px #e0e0e0;background:#fff;border:0 solid #000}@-webkit-keyframes hue{100%{-webkit-filter:hue-rotate(360deg)}}a:link:hover{-webkit-animation:hue 1s linear infinite}::-moz-selection{color:red;text-shadow:1px 1px 1px #FF99FF,2px 2px 2px #FF99FF,3px 3px 3px #FF99FF,4px 4px 4px #ff000d;font-size:16px;font-style:bold;font-family:Monotype Corsiva,Times New Roman}::selection{color:red;text-shadow:1px 1px 1px #FF99FF,2px 2px 2px #FF99FF,3px 3px 3px #FF99FF,4px 4px 4px #ff000d;font-size:16px;font-style:bold;font-family:Monotype Corsiva,Times New Roman}body{padding:0;margin:0;background-image:url(https://i.imgur.com/y2T4UHp.jpg);background-position:center center;background-repeat:no-repeat;background-attachment:fixed;background-size:cover}#TextCounter{text-align:center;color:#ffc0cb;font-size:15px;grid-column:span 4;z-index:9999999;font:20px ChunkFiveRegular;text-shadow:0 0 .2em #87F,0 0 .2em #87F,0 0 .2em #87F;font-family:'Dancing Script',cursive}anni{letter-spacing:.2px;color:#de047f;text-align:center;float:left;font-size:1.3rem;text-shadow:#fff 0 0 2px,#fff 0 0 2px,#fff 0 0 2px,#fff 0 0 2px,#fff 0 0 2px,#fff 0 0 2px,#fff 0 0 2px,#fff 0 0 2px;font-family:'Dancing Script',cursive}#TextContent{font-size:18px;grid-column:span 4;z-index:9999999;width:80%;margin:auto;overflow:hidden;max-width:80%;border:1px solid #eeeeee80;padding:7px;box-shadow:inset 1px 1px 6px 1px #eeeeee8c;background-color:#86868663;color:#eee;border-radius:5px;font-family:'Dancing Script',cursive}#TextContent font{background-image:url(https://i.imgur.com/EvfYrqj.gif);font-family:'Charm',cursive}
{% elseif app == 'js' %}
{{file_header('Content-Type: application/javascript')}}
const music = ['M%20-%20%20Iris%20Wu%20ft%20Roy%20P','My-Everything','Thế%20giới%20ảo%20tình%20yêu%20thật%20%20Trịnh%20đình%20quang%20Remix','con-mua-tinh-yeu-ha-anh-tuan-phuong-linh','mai-mai-ben-nhau-noo-phuoc-thinh'];
document.addEventListener('DOMContentLoaded', function(){
var rootTime = document.querySelector("time");
document.querySelector("anni").textContent = `${(yourDate.getDate()>9)?yourDate.getDate():"0"+yourDate.getDate()}-${(yourDate.getMonth()>8)?(yourDate.getMonth()+1):"0"+(yourDate.getMonth()+1)}-${yourDate.getFullYear()}`;
document.querySelector("date").textContent = Math.floor( Math.floor((new Date() - yourDate) / 1000) / 60 / 60 / 24)+" DAYS";
function olock(){
var today = new Date(),
hrs = (Math.floor( Math.floor((today - yourDate) / 1000) / 60 / 60)) % 24,
min = (Math.floor( Math.floor((today - yourDate) / 1000) / 60)) % 60,
sec =  Math.floor((today - yourDate) / 1000) % 60;
rootTime.textContent = `${(hrs>9)?hrs:"0"+hrs}:${(min>9)?min:"0"+min}:${(sec>9)?sec:"0"+sec}`;
}olock();
var timer = setInterval(function(){olock()}, 1000);
document.querySelector("audio").setAttribute("src", `https://kioku17.github.io/sacmau/mp3/${music[Math.floor(Math.random()*music.length)]}.mp3`);
document.getElementsByTagName("body")[0].insertAdjacentHTML("beforeend","<div id='mask'></div>");
},false);
{% else %}
	{{redirect('/error')}}
{% endif %}