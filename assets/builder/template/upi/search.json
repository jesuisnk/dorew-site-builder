{% set table_name = 'ipfs' %}
{% if get_table_count(table_name) == '0' %}
{% else %}
[
  {% for file in query_select_table(table_name,'*','ORDER BY time DESC') %}
    {% if file.id %}
    {
      "title"    : "{{file.filename}}",
      "url"      : "/view/{{file.id}}",
      "duoi"      : "{{file.filename|split('.')|last}}"
    }{% if loop.last==false %}, {% endif %}
    {% endif %}
  {% endfor %}

]
{% endif %}